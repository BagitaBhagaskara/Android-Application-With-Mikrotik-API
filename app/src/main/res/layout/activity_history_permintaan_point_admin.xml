<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".historyPermintaanPoint_admin">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:background="@color/colorPrimaryAdmin">
        <ImageView
            android:id="@+id/historyPermintaanPoint_kembali"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:src="@drawable/kembali_putih" />
    </LinearLayout>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/itemHistoryPermintaanPointUser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:listitem="@layout/row_history_permintaan_point_admin"
        android:padding="10dp"/>

</LinearLayout>